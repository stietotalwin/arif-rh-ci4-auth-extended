{
    "name": "stietotalwin/arif-rh-ci4-auth-extended",
    "description": "PHPAuth for CodeIgniter 4",
    "homepage": "https://github.com/stietotalwin/arif-rh-ci4-auth-extended",
    "keywords": [
        "auth",
        "codeigniter 4 auth",
        "authentication",
        "phpauth"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arif Rahman Hakim feat. Stie Totalwin",
            "email": "arifrahmanhakim.net@gmail.com"
        }
    ],
    "require": {
        "arif-rh/ci4-dynamic-model": "^0.3",
        "bjeavons/zxcvbn-php": "^1.0"
    },
    "require-dev": {
        "codeigniter4/framework": "^4.1",
        "phpunit/phpunit": "8.5.*"
    },
    "autoload": {
        "psr-4": {
            "Arifrh\\Auth\\": "src"
        }
